{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "outDir": "dist"
  }
}
